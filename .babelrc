{
    "compact": false,
    "presets": [
        "@babel/preset-react",
        "@babel/preset-flow",
        "@babel/preset-env"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "styled-components",
        "react-docgen",
        "relay",
        "transform-class-properties",
        [
            "react-intl",
            {
                "messagesDir": "./intl/react/"
            }
        ]
    ]
}
