    "babel-plugin-relay": "^11.0.2",
    "loader-utils": "^2.0.0",
    "redis": "^4.0.0-next.5",
    "cookie": "^0.4.1",
    "await-exec": "^0.1.2",
    "next": "10",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.14",
    "eslint": "^7.30.0",
    "local-ssl-proxy": "1.3.0",
    "eslint-config-next": "^11.0.1",
    "typescript": "^4.3.5"
