        {
            "type": "vcs",
            "url": "https://github.com/spyl94/HWIOAuthBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/emgiezet/FOSElasticaBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/spyl94/behat-parallel-scenario"
        },
        {
            "type": "vcs",
            "url": "https://github.com/spyl94/BehatPageObjectExtension"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "~2.8",
        "doctrine/orm": "^2.4.8",
        "doctrine/dbal": "^2.4.4",
        "doctrine/doctrine-bundle": "~1.6",
        "twig/extensions": "~1.0",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.9",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.14",

