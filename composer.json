{
    "name": "capco/symfony2",
    "license": "proprietary",
    "type": "project",
    "description": "Cap-Collectif Symfony2",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ArteGEIE/serializer"
        },
        {
            "type": "vcs",
            "url": "https://github.com/spyl94/HWIOAuthBundle"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "2.7.*",
        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/dbal": "<2.5",
        "doctrine/doctrine-bundle": "~1.4",
        "twig/extensions": "~1.0",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",

        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "stof/doctrine-extensions-bundle": "~1.1",
        "hautelook/alice-bundle": "~0.2",

        "elao/web-profiler-extra-bundle": "~2.3",

        "sonata-project/easy-extends-bundle": "~2.1",
        "sonata-project/core-bundle": "~2.3",
        "sonata-project/admin-bundle": "~2.3@dev",
        "sonata-project/doctrine-orm-admin-bundle": "~2.3@dev",
        "sonata-project/block-bundle": "~2.2",
        "sonata-project/media-bundle": "dev-master#5b7da8ae4ef7cbaad971094b6d6a07dcfe67dd34",
        "sonata-project/user-bundle": "~2.2@dev",
        "sonata-project/datagrid-bundle": "~2.2@dev",
        "sonata-project/exporter": "~1.4",
        "sonata-project/classification-bundle": "~2.2@dev",

        "hwi/oauth-bundle": "dev-master#ad277911727621e09626b6ee494821a7b835c172",
        "liip/imagine-bundle": "~0.9",
        "qandidate/toggle-bundle": "~0.1",
        "predis/predis": "^1.0",
        "jolicode/jolitypo-bundle": "@stable",

        "doctrine/doctrine-migrations-bundle": "~1.0",
        "willdurand/geocoder": "~3.0",
        "tilleuls/ckeditor-sonata-media-bundle": "~1.0",
        "egeloen/ckeditor-bundle": "~3.0",
        "mewesk/twig-excel-bundle": "~1.0",
        "friendsofsymfony/elastica-bundle": "~3.1",
        "jms/serializer-bundle": "~1.0",
        "jms/serializer": "dev-fix/2-nested-serializer-calls as 1.0.0",
        "nelmio/api-doc-bundle": "~2.9",
        "friendsofsymfony/rest-bundle": "~1.7",
        "lexik/jwt-authentication-bundle": "~1.0",
        "willdurand/hateoas-bundle": "~1.0",
        "snc/redis-bundle": "1.1.x-dev",
        "ekino/newrelic-bundle": "^1.3"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "behat/behat": "~3.0.6",
        "behat/symfony2-extension": "~2.0@dev",
        "behat/mink-extension": "~2.0@dev",
        "behat/mink-selenium2-driver": "@dev",
        "behat/mink": "~1.7@dev",
        "behat/mink-browserkit-driver": "@dev",
        "sensiolabs/behat-page-object-extension": "~2.0@dev",
        "ocramius/proxy-manager": "~0.5",
        "bossa/phpspec2-expect": "*",
        "phpspec/phpspec": "~2.0",
        "guzzlehttp/guzzle": "~5.0",
        "coduo/php-matcher": "1.1.*",
        "jolicode/jolinotif": "^1.0",
        "phpunit/phpunit": "@stable",
        "pyrech/composer-changelogs": "^1.2"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "github-oauth": {"github.com": "***REMOVED***"},
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    },
    "minimum-stability": "stable"
}
