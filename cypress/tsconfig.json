{
  "compilerOptions": {
    "jsx": "preserve",
    "target": "es5",
    "module": "commonjs",
    "lib": ["dom", "es6"],
    "strict": true,
    "types": ["cypress", "node"],
    "typeRoots": ["node_modules/@types/*", "@types/*"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "~e2e-pages/*": ["./pages/*"],
      "~e2e/*": ["./*"],
      "~/*": ["../frontend/js/*"]
    }
  }
}
