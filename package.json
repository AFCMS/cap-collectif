{
  "name": "cap-collectif-SF2",
  "private": true,
  "version": "0.1.0",
  "description": "Cap-Collectif version Symfony2",
  "main": "brunch-config.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "typecheck": "./node_modules/flow-bin/cli.js check",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "build": "LOGGY_STACKS=true NODE_ENV=development ./node_modules/brunch/bin/brunch build",
    "build:watch": "LOGGY_STACKS=true NODE_ENV=development ./node_modules/brunch/bin/brunch watch",
    "build:prod": "LOGGY_STACKS=true NODE_ENV=production ./node_modules/brunch/bin/brunch build --production",
    "build-server-bundle": "LOGGY_STACKS=true NODE_ENV=development ./node_modules/webpack/bin/webpack.js --config webpack.config.serverside.js",
    "build-server-bundle:prod": "LOGGY_STACKS=true NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config webpack.config.serverside.js",
    "checkcs": "./node_modules/eslint/bin/eslint.js app/Resources/js",
    "lint": "./node_modules/eslint/bin/eslint.js --cache --fix app/Resources/js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jolicode/CapCollectif-SF2"
  },
  "author": "lbrunet@jolicode.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jolicode/CapCollectif-SF2/issues"
  },
  "jest": {
    "testPathDirs": [
      "app/Resources/js"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "\\-test.js$",
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-enzyme"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  },
  "homepage": "https://github.com/jolicode/CapCollectif-SF2",
  "dependencies": {
    "autosize": "^3.0.15",
    "babel-brunch": "^6.0.6",
    "babel-core": "^6.20.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.20.0",
    "babel-template": "^6.14.0",
    "bower": "^1.7.7",
    "brunch": "2.9.1",
    "classnames": "^2.2.3",
    "clean-css-brunch": "^2.0.0",
    "css-brunch": "^2.0.0",
    "diff": "^3.0.0",
    "exports-loader": "^0.6.3",
    "fancybox": "^3.0.0",
    "flat": "^2.0.1",
    "flux": "^2.1.1",
    "imports-loader": "^0.7.0",
    "javascript-brunch": "^2.0.0",
    "jquery": "^3.1.0",
    "json-brunch": "^1.5.4",
    "mailcheck": "^1.1.1",
    "moment": "^2.11.2",
    "normalizr": "^2.3.0",
    "process-env-brunch": "^1.4.4-d",
    "qs": "^6.3.0",
    "quill": "^0.20.1",
    "react": "^15.3.1",
    "react-addons-shallow-compare": "^15.4.1",
    "react-bootstrap": "^0.29.5",
