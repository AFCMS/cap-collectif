{
  "name": "platform",
  "private": true,
  "scripts": {
    "test": "jest --notify",
    "test:ci": "jest --coverage --ci --runInBand",
    "test:watch": "jest --watchAll",
    "flow": "flow",
    "flow:coverage": "flow-coverage-report",
    "build": "NODE_ENV=development ./node_modules/brunch/bin/brunch build",
    "build:watch": "NODE_ENV=development ./node_modules/brunch/bin/brunch watch",
    "build:prod": "NODE_ENV=production ./node_modules/brunch/bin/brunch build -p",
    "build-server-bundle": "NODE_ENV=development ./node_modules/webpack/bin/webpack.js --config webpack.config.serverside.js",
    "build-server-bundle:prod": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config webpack.config.serverside.js",
    "build-relay-schema": "relay-compiler --src ./app/Resources/js --schema schema.graphql --noFutureProofEnums",
    "build-relay-schema:watch": "relay-compiler --watch --src ./app/Resources/js --schema schema.graphql --noFutureProofEnums",
    "download-translations": "bin/console translation:download --no-debug",
    "update-js-translation": "babel-node ./xlf2-to-json.js",
    "trad": "yarn run download-translations && yarn run update-js-translation",
    "clean-cache": "rm -rf var/cache/*",
    "compile-graphql": "bin/console graphql:compile --no-debug && composer dump-autoload",
    "dump-graphql-schema": "bin/console graphql:dump-schema --no-debug --file schema.graphql --format graphql",
    "relay": "yarn run clean-cache && yarn run compile-graphql && yarn run dump-graphql-schema && yarn run build-relay-schema",
    "relay:watch": "yarn run clean-cache && yarn run compile-graphql && yarn run dump-graphql-schema && yarn run build-relay-schema:watch",
    "checkcs": "eslint --cache app/Resources/js",
    "lint": "eslint --cache --fix app/Resources/js",
    "bundlesize": "bundlesize",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:static": "build-storybook -c .storybook -o docs",
    "prettier": "prettier --write 'app/Resources/js/**/*.js'",
    "precommit": "flow status && lint-staged"
  },
  "pre-commit": "precommit",
  "flow-coverage-report": {
    "threshold": 70,
    "outputDir": "coverage/flow/",
    "includeGlob": [
      "app/Resources/js/**/*.js"
    ],
    "type": [
      "text",
      "html",
      "json"
    ]
  },
  "lint-staged": {
    "app/Resources/js/**/*.js": [
      "eslint --fix",
      "prettier --parser flow --write",
      "jest --bail --findRelatedTests",
      "git add"
    ],
    "features/**/*.feature": [
      "gherkin-lint"
    ],
    "*.yml": [
      "prettier --parser yaml --write"
    ],
    "*.php": [
      "prettier --parser php --write",
      "git add",
      "php -d memory_limit=-1 bin/phpstan analyse -l 2 -c phpstan.neon"
    ],
    "*.py": [
      "pycodestyle --ignore=E501,W605",
      "git add"
    ],
    "*.twig": [
      "php bin/console lint:twig"
    ]
  },
  "bundlesize": [
    {
      "path": "./web/css/style.css",
      "maxSize": "60 kB"
    },
    {
      "path": "./web/css/style-admin.css",
      "maxSize": "50 kB"
    },
    {
      "path": "./web/js/app.js",
      "maxSize": "300 KB"
    },
    {
      "path": "./web/js/vendor.js",
      "maxSize": "850 KB"
    }
  ],
  "prettier": {
    "parser": "flow",
    "bracketSpacing": true,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "trailingComma": "all",
    "printWidth": 100
  },
  "jest": {
    "coverageDirectory": "./coverage/js",
    "collectCoverage": true,
    "coverageReporters": [
      "json-summary",
      "lcov"
    ],
    "roots": [
      "app/Resources/js"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "\\-test.js$",
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  },
  "dependencies": {
    "@prettier/plugin-php": "^0.6.0",
    "@storybook/addon-info": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "autosize": "^3.0.15",
    "babel-brunch": "https://github.com/babel/babel-brunch/tarball/c42c598076dcdd44f5dbe3c310f78e06e992c6e1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-docgen": "^2.0.0-rc.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-relay": "^1.6.0",
    "babel-plugin-styled-components": "^1.5.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babel-template": "^6.25.0",
    "bower": "^1.7.7",
    "brunch": "2.10.10",
    "bundlesize": "^0.15.3",
    "classnames": "^2.2.3",
    "clean-css-brunch": "^2.10.0",
    "codecov": "^3.0.4",
    "copycat-brunch": "^1.1.0",
    "create-react-class": "^15.6.3",
    "diff": "^3.3.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^3.19.0",
    "eslint-brunch": "^3.12.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-flowtype": "^2.46.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.4.0-rc.1",
    "eslint-plugin-relay": "^0.0.24",
    "exports-loader": "^0.6.3",
    "fancybox": "^3.0.0",
