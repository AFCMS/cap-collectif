    "flow-bin": "^0.75.0",
    "flow-coverage-report": "^0.5.0",
    "flux": "^3.1.2",
    "gherkin-lint": "^2.8.0",
    "graphql": "^0.13.2",
    "ignore-loader": "^0.1.2",
    "imports-loader": "^0.7.0",
    "jest": "^23.2.0",
    "jest-styled-components": "^4.11.0-1",
    "jquery": "^3.1.0",
    "js-cookie": "^2.2.0",
