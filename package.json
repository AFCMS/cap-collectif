{
  "name": "platform",
  "private": true,
  "scripts": {
    "test": "NODE_ENV=test jest --notify",
    "test:ci": "NODE_ENV=test jest --coverage --ci --runInBand",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "flow": "flow",
    "flow:coverage": "flow-coverage-report",
    "build": "NODE_ENV=development ./node_modules/brunch/bin/brunch build && webpack --config webpack/webpack.client.js",
    "build:analyzer": "NODE_ENV=development webpack --config webpack/webpack.client.analyzer.js",
    "build:analyzer:prod": "NODE_ENV=production webpack --config webpack/webpack.prod.analyzer.js",
    "build-legacy:watch": "NODE_ENV=development ./node_modules/brunch/bin/brunch watch",
    "build:watch": "NODE_ENV=development webpack --config webpack/webpack.client.hot.js",
    "build:prod": "NODE_ENV=production ./node_modules/brunch/bin/brunch build -p && webpack --config webpack/webpack.prod.client.js",
    "build-server-bundle": "NODE_ENV=development ./node_modules/webpack/bin/webpack.js --config webpack/webpack.config.serverside.legacy.js",
    "build-server-bundle:prod": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config webpack/webpack.config.serverside.legacy.js",
    "build-relay-schema": "relay-compiler --src ./app/Resources/js --schema schema.internal.graphql --noFutureProofEnums",
    "build-relay-schema:watch": "relay-compiler --watch --src ./app/Resources/js --schema schema.internal.graphql --noFutureProofEnums",
    "download-translations": "bin/console translation:download --no-debug",
    "update-js-translation": "node --experimental-modules ./xlf2-to-json.mjs",
    "trad": "yarn run download-translations && yarn run update-js-translation",
    "fonts": "cd app/Resources; rm -rf fonts; git clone --depth 1 git@github.com:cap-collectif/fonts.git; cp -r fonts/fonts/. assets/fonts",
    "compile-graphql": "fab local.app.cmd:'graphql:compile --no-debug' && composer dump-autoload",
    "dump-graphql-schemas": "fab local.qa.graphql_schemas",
    "clean-cache": "rm -rf var/cache/*",    
    "generate-graphql-files": "yarn run clean-cache && yarn run compile-graphql && yarn run dump-graphql-schemas",
    "relay": "yarn run generate-graphql-files && yarn run build-relay-schema",
    "relay:watch": "yarn run generate-graphql-files && yarn run build-relay-schema:watch",
    "graphql:lint:public": "graphql-schema-linter --comment-descriptions schema.public.graphql",
    "graphql:lint:internal": "graphql-schema-linter --comment-descriptions schema.internal.graphql",
    "lint": "eslint --cache --fix app/Resources/js",
    "bundlesize": "bundlesize",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy-storybook": "rm -fr .out && yarn run build-storybook -c .storybook -o .out && mkdir -p .out/.circleci && touch .out/.circleci/config.yml && echo 'version: 2\njobs:\n   build:\n      machine: true\n      branches:\n         ignore: gh-pages\n      steps:\n         - run: echo Skipping tests on gh-pages branch' > '.out/.circleci/config.yml'  && echo 'ui.cap-collectif.com' > '.out/CNAME' && storybook-to-ghpages --existing-output-dir=.out",
    "prettier:js": "prettier --parser flow --write 'app/Resources/js/**/*.js'",
    "prettier:php": "prettier --parser php --write 'src/**/*.php'",
    "prettier": "yarn run prettier:js",
    "precommit": "flow status && lint-staged"
  },
  "pre-commit": "precommit",
  "flow-coverage-report": {
    "threshold": 70,
    "outputDir": "coverage/flow/",
    "globIncludePatterns": [
      "app/Resources/js/**/*.js"
    ],
    "reportTypes": [
      "text",
      "html",
      "json"
    ]
  },
  "graphql-doctor": {
    "schema.public.graphql": {
      "ref": "heads/master",
      "schemaPath": "schema.public.graphql"
    },
    "schema.preview.graphql": {
      "ref": "heads/master",
      "schemaPath": "schema.preview.graphql"
    },
    "schema.internal.graphql": {
      "ref": "heads/master",
      "schemaPath": "schema.internal.graphql"
    }
  },
  "lint-staged": {
    "schema.public.graphql": [
      "graphql-schema-linter --comment-descriptions"
    ],
    "app/Resources/js/**/*.js": [
      "eslint --fix",
      "prettier --parser flow --write",
      "jest --bail --findRelatedTests",
      "git add"
    ],
    "features/**/*.feature": [
      "gherkin-lint"
    ],
    "*.yml": [
      "prettier --parser yaml --write"
    ],
    "*.php": [
      "./bin/php-cs-fixer fix --config=.php_cs --verbose",
      "prettier --parser php --write",
      "git add",
      "php -d memory_limit=-1 bin/phpstan analyse -l 2 -c phpstan.neon"
    ],
    "*.py": [
      "pycodestyle --ignore=E501,W605",
      "git add"
    ],
    "*.twig": [
      "php bin/console lint:twig"
    ]
  },
  "bundlesize": [
    {
      "path": "./web/css/style.css",
      "maxSize": "60 kB"
    },
    {
      "path": "./web/css/style-admin.css",
      "maxSize": "50 kB"
    },
    {
      "path": "./web/js/app.js",
      "maxSize": "1010 KB"
    },
    {
      "path": "./web/js/vendor.js",
      "maxSize": "230 KB"
    }
  ],
  "prettier": {
    "parser": "flow",
    "bracketSpacing": true,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "trailingComma": "all",
    "printWidth": 100
  },
