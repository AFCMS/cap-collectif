{
  "name": "cap-collectif-SF2",
  "private": true,
  "version": "0.1.0",
  "description": "Cap-Collectif version Symfony2",
  "main": "brunch-config.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha --opts mocha.opts",
    "test:watch": "NODE_ENV=test ./node_modules/mocha/bin/mocha --opts mocha.opts --watch --reporter min",
    "build": "NODE_ENV=development ./node_modules/brunch/bin/brunch build",
    "build:watch": "NODE_ENV=development ./node_modules/brunch/bin/brunch watch",
    "build:prod": "NODE_ENV=production ./node_modules/brunch/bin/brunch build --production",
    "build-server-bundle": "NODE_ENV=development ./node_modules/webpack/bin/webpack.js --config webpack.config.serverside.js",
    "build-server-bundle:prod": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config webpack.config.serverside.js",
    "checkcs": "./node_modules/eslint/bin/eslint.js app/Resources/js && ./node_modules/jscs/bin/jscs app/Resources/js/translations/*.js",
    "lint": "./node_modules/eslint/bin/eslint.js --cache --fix  app/Resources/js && ./node_modules/jscs/bin/jscs --fix app/Resources/js/translations/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jolicode/CapCollectif-SF2"
  },
  "author": "lbrunet@jolicode.com",
  "license": "MIT",
