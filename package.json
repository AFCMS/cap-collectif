{
  "name": "platform",
  "private": true,
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "typecheck": "flow check",
    "typecheck:coverage": "flow-coverage-report -i 'app/Resources/js/**/*.js'",
    "build": "NODE_ENV=development ./node_modules/brunch/bin/brunch build",
    "build:watch": "NODE_ENV=development ./node_modules/brunch/bin/brunch watch",
    "build:prod": "NODE_ENV=production ./node_modules/brunch/bin/brunch build --production",
    "build-server-bundle": "NODE_ENV=development ./node_modules/webpack/bin/webpack.js --config webpack.config.serverside.js",
    "build-server-bundle:prod": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config webpack.config.serverside.js",
    "build-relay-schema": "relay-compiler --src ./app/Resources/js --schema schema.graphql",
    "build-relay-schema:watch": "relay-compiler --watch --src ./app/Resources/js --schema schema.graphql",
    "dump-graphql-schema": "bin/console graphql:dump-schema --file schema.graphql --format graphqls",
    "relay": "yarn run dump-graphql-schema && yarn run build-relay-schema",
    "relay:watch": "yarn run dump-graphql-schema && yarn run build-relay-schema:watch",
    "checkcs": "eslint app/Resources/js",
    "lint": "eslint --cache --fix app/Resources/js",
    "prettier": "prettier --write --bracket-spacing --single-quote --jsx-bracket-same-line --trailing-comma all --print-width 80 \"app/Resources/js/**/*.js\"",
    "precommit": "flow status && lint-staged"
  },
  "pre-commit": "precommit",
  "lint-staged": {
    "app/Resources/js/**/*.js": [
      "eslint --fix",
      "prettier --write --bracket-spacing --single-quote --jsx-bracket-same-line --trailing-comma all --print-width 80",
      "git add"
    ],
    "*.php": [
      "php-cs-fixer fix --config=.php_cs --path-mode=intersection",
      "git add"
    ],
    "*.py": [
      "autopep8 --in-place --aggressive --aggressive --ignore=E501",
      "git add"
    ],
    "*.twig": [
      "php bin/console lint:twig"
    ]
  },
  "jest": {
    "roots": [
      "app/Resources/js"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "\\-test.js$",
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-enzyme"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  },
  "dependencies": {
    "autosize": "^3.0.15",
    "babel-brunch": "https://github.com/babel/babel-brunch/tarball/c42c598076dcdd44f5dbe3c310f78e06e992c6e1",
    "babel-core": "^6.25.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-relay": "^1.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babel-template": "^6.25.0",
    "bower": "^1.7.7",
    "brunch": "^2.10.9",
    "classnames": "^2.2.3",
    "clean-css-brunch": "^2.10.0",
    "diff": "^3.3.0",
    "exports-loader": "^0.6.3",
    "fancybox": "^3.0.0",
    "flat": "^2.0.1",
    "flux": "^3.1.2",
    "google-map-react": "^0.24.0",
    "imports-loader": "^0.7.0",
    "jquery": "^3.1.0",
    "json-brunch": "^1.5.4",
    "lodash": "^4.17.4",
    "mailcheck": "^1.1.1",
    "moment": "^2.11.2",
    "points-cluster": "^0.1.4",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "quill": "^0.20.1",
    "react": "15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-async-script": "^0.9.1",
    "react-bootstrap": "^0.31.0",
    "react-checkbox-group": "https://github.com/cap-collectif/react-checkbox-group/tarball/296721a62a682c5e441bdd449704550b69257af9",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "15.4.2",
    "react-dropzone": "^3.3.4",
    "react-google-charts": "^1.0.2",
    "react-google-recaptcha": "^0.9.6",
    "react-intl": "^2.3.0",
    "react-intl-redux": "^0.6.0",
    "react-linkify": "^0.2.1",
    "react-nl2br": "^0.2.0",
    "react-on-rails": "^8.0.3",
    "react-places-autocomplete": "^5.3.1",
    "react-radio": "^2.1.1",
    "react-radio-buttons": "^1.0.11",
    "react-redux": "^5.0.3",
    "react-relay": "^1.1.0",
    "react-router": "3.0.2",
    "react-router-bootstrap": "rr-v3",
    "react-select": "1.0.0-rc.2",
    "react-sortable-hoc": "^0.6.1",
    "react-toggle": "^4.0.1",
    "react-treebeard": "https://github.com/cap-collectif/react-treebeard/tarball/6995b4123e3f3d94a4fb9200f884b3371e168628",
    "react-truncate": "^2.0.5",
    "readmore": "0.0.2",
    "redux": "^3.6.0",
    "redux-form": "^6.6.3",
    "redux-saga": "v0.14.0",
    "relay-runtime": "^1.1.0",
    "sass-brunch": "https://github.com/gavinkflam/sass-brunch/tarball/cbf2edf2646ded550c7f333cc921f295773b1d4e",
    "uglify-js-brunch": "^2.10.0",
    "webpack": "^1.13.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "enzyme": "^2.8.2",
    "eslint": "3.x",
    "eslint-brunch": "^3.12.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.50.0",
    "flow-coverage-report": "^0.3.0",
    "jest": "^20.0.4",
    "jest-serializer-enzyme": "^1.0.0",
    "lint-staged": "^4.0.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.5.3",
    "react-addons-test-utils": "15.4.2",
    "relay-compiler": "^1.1.0"
  }
}
