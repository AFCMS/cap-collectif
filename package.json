{
  "name": "platform",
  "private": true,
  "scripts": {
    "test": "jest --notify",
    "test:ci": "jest --coverage --ci --maxWorkers=4",
    "test:watch": "jest --watchAll",
    "typecheck": "flow check",
    "typecheck:coverage": "flow-coverage-report --threshold 70 -i 'app/Resources/js/**/*.js'",
    "build": "NODE_ENV=development ./node_modules/brunch/bin/brunch build",
    "build:watch": "NODE_ENV=development ./node_modules/brunch/bin/brunch watch",
    "build:prod": "NODE_ENV=production ./node_modules/brunch/bin/brunch build --production",
    "build-server-bundle": "NODE_ENV=development ./node_modules/webpack/bin/webpack.js --config webpack.config.serverside.js",
    "build-server-bundle:prod": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config webpack.config.serverside.js",
    "build-relay-schema": "relay-compiler --src ./app/Resources/js --schema schema.graphql",
    "build-relay-schema:watch": "relay-compiler --watch --src ./app/Resources/js --schema schema.graphql",
    "download-translations": "bin/console translation:download --no-debug",
    "update-js-translation": "babel-node ./xlf2-to-json.js",
    "clean-cache": "rm -rf var/cache/*",
    "trad": "yarn run clean-cache && yarn run download-translations && yarn run update-js-translation",
    "dump-graphql-schema": "bin/console graphql:dump-schema --no-debug --file schema.graphql --format graphql",
    "relay": "yarn run clean-cache && yarn run dump-graphql-schema && yarn run build-relay-schema",
    "relay:watch": "yarn run dump-graphql-schema && yarn run build-relay-schema:watch",
    "checkcs": "eslint --cache app/Resources/js",
    "lint": "eslint --cache --fix app/Resources/js",
    "bundlesize": "bundlesize",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:static": "build-storybook -c .storybook -o docs",
    "prettier": "prettier --write 'app/Resources/js/**/*.js'",
    "precommit": "flow status && lint-staged"
  },
  "pre-commit": "precommit",
  "lint-staged": {
    "app/Resources/js/**/*.js": [
      "eslint --fix",
      "prettier --write",
      "jest --findRelatedTests",
      "git add"
    ],
    "features/**/*.feature": [
      "gherkin-lint"
    ],
    "*.yml": [
      "yamllint"
    ],
    "*.php": [
      "php-cs-fixer fix --config=.php_cs --path-mode=intersection",
      "git add",
      "php -d memory_limit=-1 bin/phpstan analyse -l 2 -c phpstan.neon"
    ],
    "*.py": [
      "pycodestyle --ignore=E501",
      "git add"
    ],
    "*.twig": [
      "php bin/console lint:twig"
    ]
  },
  "bundlesize": [
    {
      "path": "./web/css/style.css",
      "maxSize": "65 kB"
    },
    {
      "path": "./web/css/style-admin.css",
      "maxSize": "50 kB"
    },
    {
      "path": "./web/js/app.js",
      "maxSize": "350 kB"
    },
    {
      "path": "./web/js/vendor.js",
      "maxSize": "1.8 MB"
    }
  ],
  "prettier": {
    "parser": "flow",
    "bracketSpacing": true,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "trailingComma": "all",
    "printWidth": 100
  },
  "jest": {
    "roots": [
      "app/Resources/js"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "\\-test.js$",
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-enzyme"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  },
  "dependencies": {
    "autosize": "^3.0.15",
    "babel-brunch": "https://github.com/babel/babel-brunch/tarball/c42c598076dcdd44f5dbe3c310f78e06e992c6e1",
    "babel-core": "^6.25.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-relay": "^1.1.0",
    "babel-plugin-styled-components": "^1.5.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babel-template": "^6.25.0",
    "bower": "^1.7.7",
    "brunch": "2.10.10",
    "classnames": "^2.2.3",
    "clean-css-brunch": "^2.10.0",
    "diff": "^3.3.0",
    "exports-loader": "^0.6.3",
    "fancybox": "^3.0.0",
    "flux": "^3.1.2",
    "gherkin-lint": "^2.8.0",
    "ignore-loader": "^0.1.2",
    "imports-loader": "^0.7.0",
    "jquery": "^3.1.0",
    "json-brunch": "^1.5.4",
    "json-loader": "^0.5.7",
    "leaflet": "^1.1.0",
    "leaflet.locatecontrol": "^0.62.0",
    "leaflet.markercluster": "^1.0.6",
    "lodash": "^4.17.4",
    "mailcheck": "^1.1.1",
    "moment": "^2.11.2",
    "moment-timezone": "^0.5.13",
    "points-cluster": "^0.1.4",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "quill": "^0.20.1",
    "react-addons-shallow-compare": "^15.4.2",
    "react-async-script": "^0.9.1",
    "react-bootstrap": "^0.31.2",
    "react-checkbox-group": "https://github.com/cap-collectif/react-checkbox-group/tarball/296721a62a682c5e441bdd449704550b69257af9",
    "react-datetime": "^2.9.0",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dropzone": "^3.3.4",
    "react-google-charts": "^1.0.2",
    "react-google-recaptcha": "^0.9.6",
    "react-intl": "https://github.com/cap-collectif/react-intl/tarball/2395c315a2b7cb87c18a14fcc46b8855becdccb0",
    "react-intl-redux": "^0.6.0",
    "react-leaflet": "^1.4.1",
    "react-leaflet-markercluster": "https://github.com/cap-collectif/react-leaflet-markercluster/tarball/2d91878f0bd99af7a5352050315a50b4c4c4bf34",
    "react-leaflet-universal": "^1.1.0",
    "react-linkify": "^0.2.1",
    "react-nl2br": "^0.2.0",
    "react-on-rails": "^8.0.3",
    "react-places-autocomplete": "^6.1.2",
    "react-radio": "^2.1.1",
    "react-radio-buttons": "^1.0.11",
    "react-redux": "^5.0.6",
    "react-relay": "^1.1.0",
    "react-router": "3.0.2",
    "react-router-bootstrap": "rr-v3",
    "react-select": "^1.0.0-rc.5",
    "react-sortable-hoc": "^0.6.1",
    "react-toggle": "^4.0.1",
    "react-treebeard": "https://github.com/cap-collectif/react-treebeard/tarball/6995b4123e3f3d94a4fb9200f884b3371e168628",
    "react-truncate": "^2.1.5",
    "readmore": "0.0.2",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-saga": "v0.14.0",
    "relay-runtime": "^1.1.0",
    "sass-brunch": "https://github.com/jukebox017/sass-brunch/tarball/793d58d32664a611794efb3894fa3e9827fae4ff",
    "styled-components": "^3.1.6",
    "styled-spinkit": "^0.2.1",
    "uglify-js-brunch": "^2.10.0",
    "webpack": "^1.13.2",
    "whatwg-fetch": "^2.0.3",
    "xml2json": "^0.11.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.3.13",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.0.4",
    "bundlesize": "^0.15.3",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-brunch": "^3.12.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.4.0-rc.1",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.66.0",
    "flow-coverage-report": "^0.3.0",
    "jest": "^22.0.4",
    "jest-serializer-enzyme": "^1.0.0",
    "jest-styled-components": "^4.11.0-1",
    "lint-staged": "^6.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.6.1",
    "react-addons-test-utils": "15.4.2",
    "relay-compiler": "^1.1.0",
    "sass-loader": "^6.0.7",
    "yaml-lint": "^1.0.0"
  },
  "version": "1.3.2"
}
