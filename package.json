    "react-google-charts": "^1.0.2",
    "react-google-recaptcha": "^0.5.2",
    "react-intl": "1.2.0",
    "react-linkify": "^0.1.1",
    "react-nl2br": "^0.2.0",
    "react-on-rails": "^6.3.2",
    "react-radio": "^2.1.1",
    "react-redux": "^5.0.1",
    "react-router": "^2.7.0",
    "react-router-bootstrap": "^0.23.1",
    "react-select": "^1.0.0-rc.2",
    "react-toggle": "^2.1.1",
    "react-truncate": "^2.0.3",
    "readmore": "0.0.2",
