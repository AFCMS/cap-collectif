{% extends 'SonataAdminBundle:CRUD:base_show_field.html.twig' %}

{% block field %}
    {%  if value | length > 0 %}
        {% for response in value %}
            <strong>{{ response.question.title }}</strong></br>
            {{ response.value | raw }}</br>
            {% if loop.last == false %}</br>{% endif %}
        {% endfor %}
    {%  endif %}
{% endblock %}
