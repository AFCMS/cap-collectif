{% if ideas|length > 0 %}
    <div class="container  block">
        <h2 class="h2">{{ 'idea.title'|trans({}, 'CapcoAppBundle') }}</h2>
        <div class="row">
            {% for idea in ideas %}
                {% include '@CapcoApp/Idea/blockIdea.html.twig' %}
            {% endfor %}
        </div>
        <a href="{{ path('app_idea') }}" class="btn  btn-primary  btn--outline">{{ 'idea.see_all'|trans({}, 'CapcoAppBundle') }}</a>
    </div>
{% endif %}


