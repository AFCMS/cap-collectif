{% extends "CapcoAppBundle::base.html.twig" %}

{% block canonical_link %}{{ url('app_page_charter', {'slug': 'charter'|trans({},'CapcoAppBundle')|lower }) }}{% endblock %}

{% block title %}
    {{ parent() }} - {{ 'charter' |trans({},'CapcoAppBundle') }}
{% endblock %}

{% block jumbotron %}
    <div class="jumbotron  jumbotron--custom  jumbotron--bg-1  text-center">
        <div class="container">
            <h1>{{ 'charter' |trans({},'CapcoAppBundle') }} </h1>
        </div>
    </div>
{% endblock %}

{% block body %}
    <div class="container  container--thinner">
        {{ body|raw }}
    </div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
{% endblock %}
